<COMPONENT>
	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="1.0"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="Benoit Courtine"
		RELEASE_DATE="20100728A"
		STARTABLE="false"
		SCHEMA_AUTO_PROPAGATE="false"
		VISIBLE="false"
		TECHNICAL="true"
	>
		<SIGNATURE></SIGNATURE>
	</HEADER>

	<FAMILIES>
		<FAMILY>Technical</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="DESTINATION" FIELD="TEXT" NUM_ROW="2" REQUIRED="true">
			<DEFAULT>tAggregate_1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUPBYS" FIELD="TABLE" NUM_ROW="4" NB_LINES="5">
			<ITEMS>
				<ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
				<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="OPERATIONS" FIELD="TABLE" REQUIRED="false" NUM_ROW="3" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
				<ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="COUNT">
						<ITEM NAME="COUNT" VALUE="count" />
						<ITEM NAME="MIN" VALUE="min" />
						<ITEM NAME="MAX" VALUE="max" />
						<ITEM NAME="AVG" VALUE="avg" />
						<ITEM NAME="SUM" VALUE="sum" />
						<ITEM NAME="FIRST" VALUE="first" />
						<ITEM NAME="LAST" VALUE="last" />
						<ITEM NAME="LIST" VALUE="list" />
						<ITEM NAME="LIST_OBJECT" VALUE="list_object" />
						<ITEM NAME="DISTINCT" VALUE="distinct" />
						<ITEM NAME="STD_DEV" VALUE="std_dev" />
						<ITEM NAME="AND" VALUE="and" />
						<ITEM NAME="OR" VALUE="or" />
						<ITEM NAME="XOR" VALUE="xor" />
					</ITEMS>
				</ITEM>
				<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
				<ITEM NAME="IGNORE_NULL" FIELD="CHECK" />
			</ITEMS>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="LIST_DELIMITER" FIELD="TEXT" REQUIRED="false" NUM_ROW="1">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_FINANCIAL_PRECISION" FIELD="CHECK" REQUIRED="true" NUM_ROW="2">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CHECK_TYPE_OVERFLOW" FIELD="CHECK" REQUIRED="true" NUM_ROW="3">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CHECK_ULP" FIELD="CHECK" REQUIRED="true" NUM_ROW="4">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION />

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
